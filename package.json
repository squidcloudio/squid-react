{
  "name": "@squidcloud/react",
  "version": "1.0.41",
  "description": "Squid React SDK",
  "types": "dist/cjs/index.d.ts",
  "main": "dist/cjs/index.js",
  "scripts": {
    "build": "rollup -c && yalc push",
    "lint": "eslint ./src",
    "format": "prettier --write \"./**/src/**/*{ts,js,tsx,jsx,md,mdx}\"",
    "clean": "rimraf node_modules dist",
    "update": "ncu @squidcloud/client @squidcloud/common -u",
    "publish:public": "npm run build && bash ./publish.sh",
    "publish:beta": "npm run build && bash ./publish.sh beta"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "Squid",
    "Squid Cloud",
    "react"
  ],
  "peerDependencies": {
    "react": "^16.11.0 || ^17 || ^18",
    "react-dom": "^16.11.0 || ^17 || ^18",
    "rxjs": ">=7.5.7 <8.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/node": "^18.14.4",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-next": "13.4.12",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "magic-string": "^0.30.5",
    "npm-check-updates": "^16.10.8",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.18.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preserve-directives": "^0.2.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "yalc": "^1.0.0-pre.53"
  },
  "dependencies": {
    "@squidcloud/client": "^1.0.150",
    "@squidcloud/common": "^1.0.150",
    "assertic": "^1.0.0"
  }
}
